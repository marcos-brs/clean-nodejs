{
  "name": "clean-nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:zerocoolbr/clean-nodejs.git",
  "author": "Marcos Santana <brmarcossantanabr@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node release/index.js",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node -r tsconfig-paths/register --transpile-only' ./src/index.ts",
    "build": "rimraf release && tsc -p tsconfig-build.json",
    "lint": "eslint --fix 'src/**/*.ts' 'tests/**/*.ts'",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:unit": "npm run test -- --watch -c jest-unit-config.js",
    "test:integration": "npm run test -- --watch -c jest-integration-config.js",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:ci": "npm run test -- --coverage",
    "prepare-husky": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/bcrypt": "^5.0.0",
    "@types/bunyan-format": "^0.2.3",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.3",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript-prettier": "^4.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.3",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.6.0",
    "@types/mongoose": "<=5.10.18",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "<=5.10.18",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.6",
    "tsyringe": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "_moduleAliases": {
    "@": "release"
  }
}
